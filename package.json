{
  "name": "obsidian-clipper",
  "displayName": "Obsidian clipper",
  "version": "0.0.4",
  "description": "A small tool can import web page content into Obsidian",
  "author": "sx.ma223@gmail.com",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "test": "plasmo test"
  },
  "dependencies": {
    "@ant-design/icons": "^5.2.6",
    "@tehshrike/readability": "^0.2.0",
    "antd": "^5.12.2",
    "dayjs": "^1.11.10",
    "i18next": "^23.7.18",
    "jsdom": "^23.0.1",
    "plasmo": "0.84.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "^14.0.1",
    "react-intl-universal": "^2.9.1",
    "turndown": "^7.1.2"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.251",
    "@types/node": "20.9.0",
    "@types/react": "18.2.37",
    "@types/react-dom": "18.2.15",
    "prettier": "3.0.3",
    "typescript": "5.2.2"
  },
  "pnpm": {
    "supportedArchitectures": {
      "os": [
        "win32",
        "darwin",
        "current"
      ],
      "cpu": [
        "x64",
        "arm64"
      ]
    }
  },
  "manifest": {
    "permissions": [
      "storage",
      "contextMenus",
      "activeTab"
    ],
    "commands": {
      "import-article": {
        "suggested_key": {
          "default": "Ctrl+X",
          "mac": "Command+X"
        },
        "description": "Import web content"
      }
    }
  }
}
