{
  "extends": "plasmo/templates/tsconfig.base",
  "exclude": [
    "node_modules"
  ],
  "include": [
    ".plasmo/index.d.ts",
    ".plasmo/**/*",
    "./**/*.ts",
    "./**/*.tsx",
    "./**/*.js"],
  "compilerOptions": {
    "paths": {
      "~*": [
        "./src/*"
      ],
      "@Utils/*": ["./src/utils/*"],
    },
    "baseUrl": "."
  },
}
